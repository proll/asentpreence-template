<div class="overlay__slides">
	{.repeated section items}
	<div data-id="{id}" class="overlay__slide"{.section assetUrl} style="background-image:url({@})"{.end}{.section items} data-bg="{items[0].colorData.suggestedBgColor}"{.end}>
		{.section title}
		<h2>{@}</h2>
		{.end}
		<span class="overlay__desc">
			{.section excerpt}<h3 class="overlay__desc-h">{@}</h3>{.end}
			{.section body}<span class="overlay__desc-misc">{@}</span>{.end}
		</span>
	</div>
	{.end}
</div>